@model IEnumerable<_4._0_GetPost.Models.Alcohol>
@{
    ViewData["Title"] = "Index";
}

<!DOCTYPE html>

<html>
<head>
    <title>Магазин Алкоголя</title>
</head>
<body>
    <h3>Напитки</h3>
    <table class="table-condensed">
        <tr>
            <td>Бренд</td>
            <td>Тип</td>
        </tr>
        @foreach (var alc in Model)
        {
            <tr>
                <td>@alc.Brand</td>
                <td>@alc.Type</td>
                <td><a href="~Alcohol/Properties/@alc.Id">Подробнее</a></td>
            </tr>
        }
    </table>
</body>
</html>

